{
    "name": "atlas/orm",
    "type": "library",
    "description": "ORM: No migrations. No annotations. No lazy loading. No domain models. No data-type abstractions. No behaviors. No opinions. Just data mapping.",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Atlas",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Atlas/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "aura/sql": "~2.0",
        "aura/sqlquery": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Orm\\": "src/"
        }
    },
    "require-dev": {
        "atlas/cli": "0.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Orm\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
